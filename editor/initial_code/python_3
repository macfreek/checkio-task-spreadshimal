def checkio(n):
    """
        Return the label of the n-th spreadsheet column.
    """
    return 'A'

if __name__ == '__main__':
    #These "asserts" using only for self-checking and not necessary for auto-testing
    assert checkio(1) == 'A'
    assert checkio(2) == 'B'
    assert checkio(26) == 'Z'
    assert checkio(27) == 'AA'
    assert checkio(53) == 'BA'
    assert checkio(1000) == 'ALL'
    assert checkio(0) == ''
    assert checkio(-1000) == '-ALL'
    print('Done')
